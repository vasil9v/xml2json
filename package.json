  "main": "lib/main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "mocha -r blanket -R html-cov test",
    "lcov": "mocha -r blanket -R mocha-lcov-reporter",
    "test": "mocha test"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/vasil9v/xml2json.git"
  },
  "dependencies": {
    "xml2js": ""
  },
  "devDependencies": {
    "blanket": "1.1.6",
    "codeclimate-test-reporter": "0.0.4",
    "mocha": "2.1.0",
    "mocha-lcov-reporter": "0.0.1"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "pattern": [ ]
